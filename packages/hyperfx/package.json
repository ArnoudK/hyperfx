{
  "name": "hyperfx",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "BSD-2-Clause",
  "private": "false",
  "type": "module",
  "sideEffects": false,
  "description": "Reactive JSX framework with signals, SSR, and optimizing compiler",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./compiler": {
      "types": "./dist/compiler/index.d.ts",
      "default": "./dist/compiler/index.js"
    },
    "./vite": {
      "types": "./dist/compiler/vite.d.ts",
      "default": "./dist/compiler/vite.js"
    },
    "./webpack": {
      "types": "./dist/compiler/webpack.d.ts",
      "default": "./dist/compiler/webpack.js"
    },
    "./rollup": {
      "types": "./dist/compiler/rollup.d.ts",
      "default": "./dist/compiler/rollup.js"
    },
    "./esbuild": {
      "types": "./dist/compiler/esbuild.d.ts",
      "default": "./dist/compiler/esbuild.js"
    },
    "./rspack": {
      "types": "./dist/compiler/rspack.d.ts",
      "default": "./dist/compiler/rspack.js"
    },
    "./jsx": {
      "types": "./dist/jsx/jsx-runtime.d.ts",
      "default": "./dist/jsx/jsx-runtime.js"
    },
    "./jsx-server": {
      "types": "./dist/jsx/jsx-server-runtime.d.ts",
      "default": "./dist/jsx/jsx-server-runtime.js"
    },
    "./jsx-server/jsx-runtime": {
      "types": "./dist/jsx/jsx-server-runtime.d.ts",
      "default": "./dist/jsx/jsx-server-runtime.js"
    },
    "./jsx-server/jsx-dev-runtime": {
      "types": "./dist/jsx/jsx-server-runtime.d.ts",
      "default": "./dist/jsx/jsx-server-runtime.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx/jsx-runtime.d.ts",
      "default": "./dist/jsx/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx/jsx-dev-runtime.d.ts",
      "default": "./dist/jsx/jsx-dev-runtime.js"
    },
    "./jsx-server-runtime": {
      "types": "./dist/jsx/jsx-server-runtime.d.ts",
      "default": "./dist/jsx/jsx-server-runtime.js"
    },
    "./jsx/jsx-runtime": {
      "types": "./dist/jsx/jsx-runtime.d.ts",
      "default": "./dist/jsx/jsx-runtime.js"
    },
    "./jsx/jsx-dev-runtime": {
      "types": "./dist/jsx/jsx-dev-runtime.d.ts",
      "default": "./dist/jsx/jsx-dev-runtime.js"
    },
    "./jsx-runtime/jsx-dev-runtime": {
      "types": "./dist/jsx/jsx-dev-runtime.d.ts",
      "default": "./dist/jsx/jsx-dev-runtime.js"
    },
    "./runtime-dom": {
      "types": "./dist/runtime-dom/index.d.ts",
      "default": "./dist/runtime-dom/index.js"
    }
  },
  "author": {
    "name": "Arnoud Kerkhof"
  },
  "keywords": [
    "framework",
    "web components",
    "dom",
    "jsx",
    "reactive",
    "signals",
    "ssr",
    "compiler",
    "vite",
    "unplugin"
  ],
  "version": "0.1.0",
  "scripts": {
    "lint": "oxlint --fix src",
    "build": "tsc -b",
    "typecheck": "tsc --noEmit",
    "dev": "tsc -b --watch --preserveWatchOutput",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "release": "pnpm run build && changeset publish",
    "prepublish": "pnpm build"
  },
  "devDependencies": {
    "@effect/platform": "^0.94.2",
    "@eslint/js": "^9.28.0",
    "@types/babel__core": "^7.20.0",
    "@types/babel__traverse": "^7.20.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.31",
    "@vitest/ui": "^4.0.16",
    "effect": "^3.11.0",
    "happy-dom": "^20.0.11",
    "jsdom": "^26.1.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.2",
    "typescript": "^5.8.3",
    "vitest": "^4.0.16"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@babel/types": "^7.24.0",
    "magic-string": "^0.30.0",
    "unplugin": "^3.0.0"
  },
  "peerDependencies": {
    "effect": "^3.11.0",
    "@effect/platform": "^0.94.0"
  },
  "peerDependenciesMeta": {
    "@effect/platform": {
      "optional": true
    }
  }
}
