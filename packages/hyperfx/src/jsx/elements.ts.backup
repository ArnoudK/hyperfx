import type { ReactiveValue, JSXElement } from './jsx-runtime';

// Re-export for convenience
export type { ReactiveValue } from './jsx-runtime';

// Base reactive types for common primitives
export type ReactiveString = ReactiveValue<string>;
export type ReactiveNumber = ReactiveValue<number>;
export type ReactiveBoolean = ReactiveValue<boolean>;
export type ReactiveObject<T = Record<string, any>> = ReactiveValue<T>;

// ========================================
// GLOBAL ATTRIBUTES - VALID ON ALL HTML ELEMENTS
// ========================================

export interface GlobalHTMLAttributes {
  // Core attributes
  id?: ReactiveString;
  class?: ReactiveString;
  style?: ReactiveValue<string | Partial<CSSStyleDeclaration>>;
  title?: ReactiveString;
  lang?: ReactiveString;
  dir?: ReactiveString;
  contenteditable?: ReactiveBoolean | ReactiveString;
  draggable?: ReactiveBoolean | ReactiveString;
  hidden?: ReactiveBoolean;
  inert?: ReactiveBoolean;
  spellcheck?: ReactiveBoolean | ReactiveString;
  tabindex?: ReactiveNumber | ReactiveString;
  translate?: ReactiveString;

  // Content attributes (for text manipulation)
  innerHTML?: ReactiveString;
  textContent?: ReactiveString;

  // ARIA attributes
  role?: ReactiveString;
  'aria-activedescendant'?: ReactiveString;
  'aria-atomic'?: ReactiveBoolean | ReactiveString;
  'aria-autocomplete'?: ReactiveString;
  'aria-busy'?: ReactiveBoolean | ReactiveString;
  'aria-checked'?: ReactiveBoolean | ReactiveString;
  'aria-colcount'?: ReactiveNumber | ReactiveString;
  'aria-colindex'?: ReactiveNumber | ReactiveString;
  'aria-colspan'?: ReactiveNumber | ReactiveString;
  'aria-controls'?: ReactiveString;
  'aria-current'?: ReactiveString;
  'aria-describedby'?: ReactiveString;
  'aria-details'?: ReactiveString;
  'aria-disabled'?: ReactiveBoolean | ReactiveString;
  'aria-errormessage'?: ReactiveString;
  'aria-expanded'?: ReactiveBoolean | ReactiveString;
  'aria-flowto'?: ReactiveString;
  'aria-grabbed'?: ReactiveBoolean | ReactiveString;
  'aria-haspopup'?: ReactiveString;
  'aria-hidden'?: ReactiveBoolean | ReactiveString;
  'aria-invalid'?: ReactiveString;
  'aria-keyshortcuts'?: ReactiveString;
  'aria-label'?: ReactiveString;
  'aria-labelledby'?: ReactiveString;
  'aria-level'?: ReactiveNumber | ReactiveString;
  'aria-live'?: ReactiveString;
  'aria-modal'?: ReactiveBoolean | ReactiveString;
  'aria-multiline'?: ReactiveBoolean | ReactiveString;
  'aria-multiselectable'?: ReactiveBoolean | ReactiveString;
  'aria-orientation'?: ReactiveString;
  'aria-owns'?: ReactiveString;
  'aria-placeholder'?: ReactiveString;
  'aria-posinset'?: ReactiveNumber | ReactiveString;
  'aria-pressed'?: ReactiveBoolean | ReactiveString;
  'aria-readonly'?: ReactiveBoolean | ReactiveString;
  'aria-relevant'?: ReactiveString;
  'aria-required'?: ReactiveBoolean | ReactiveString;
  'aria-roledescription'?: ReactiveString;
  'aria-rowcount'?: ReactiveNumber | ReactiveString;
  'aria-rowindex'?: ReactiveNumber | ReactiveString;
  'aria-rowspan'?: ReactiveNumber | ReactiveString;
  'aria-selected'?: ReactiveBoolean | ReactiveString;
  'aria-setsize'?: ReactiveNumber | ReactiveString;
  'aria-sort'?: ReactiveString;
  'aria-valuemax'?: ReactiveNumber | ReactiveString;
  'aria-valuemin'?: ReactiveNumber | ReactiveString;
  'aria-valuenow'?: ReactiveNumber | ReactiveString;
  'aria-valuetext'?: ReactiveString;

  // Data attributes
  'data-*'?: ReactiveString;

  // Event handlers (non-reactive)
  onclick?: (event: MouseEvent) => void;
  oninput?: (event: InputEvent) => void;
  onchange?: (event: Event) => void;
  onsubmit?: (event: SubmitEvent) => void;
  onfocus?: (event: FocusEvent) => void;
  onblur?: (event: FocusEvent) => void;
  onkeydown?: (event: KeyboardEvent) => void;
  onkeyup?: (event: KeyboardEvent) => void;
  onkeypress?: (event: KeyboardEvent) => void;
  onmouseenter?: (event: MouseEvent) => void;
  onmouseleave?: (event: MouseEvent) => void;
  onmousemove?: (event: MouseEvent) => void;
  onmousedown?: (event: MouseEvent) => void;
  onmouseup?: (event: MouseEvent) => void;
  ontouchstart?: (event: TouchEvent) => void;
  ontouchend?: (event: TouchEvent) => void;
  ontouchmove?: (event: TouchEvent) => void;
  onscroll?: (event: Event) => void;
  onresize?: (event: Event) => void;
  onload?: (event: Event) => void;
  onerror?: (event: Event) => void;
}

// ========================================
// FORM-ASSOCIATED ATTRIBUTES - SHARED BY FORM ELEMENTS
// ========================================

export interface FormAssociatedAttributes {
  disabled?: ReactiveBoolean;
  form?: ReactiveString;
  name?: ReactiveString;
}

